{
  "build": {
    "arduino":{
      "ldscript": "nrf52811_xxaa.ld"
    },
    "core": "nRF5",
    "cpu": "cortex-m4",
    "extra_flags": "-DARDUINO_HY_2000x -DNRF52811_XXAA -DCONFIG_GPIO_AS_PINRESET",
    "f_cpu": "64000000L",
    "mcu": "nrf52811",
    "variant": "HY_2000x"
    },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "default_tools": [
      "jlink"
    ],
    "jlink_device": "nRF52811_xxAA",
    "onboard_tools": [
      "cmsis-dap",
      "jlink"
    ],
    "svd_path": "nrf52811.svd"
  },
  "frameworks": [
    "arduino"
   ],
  "name": "Holyiot 2000x",
  "upload": {
    "maximum_ram_size": 65536,
    "maximum_size": 196608,
    "protocol": "stlink",
    "protocols": [
      "jlink",
      "nrfjprog",
      "stlink",
      "blackmagic",
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/Nordic-nRF52-DK/",
  "vendor": "Nordic"
}
